#include <iostream>
using namespace std;

int main() {
    // Input values
    int hours = 7;       
    int minutes = 12;   

    int totalMinutes = (hours * 60) + minutes;

    cout << "Input hours: " << hours << endl;
    cout << "Input minutes: " << minutes << endl;
    cout << "Total: " << totalMinutes << " minutes" << endl;

    return 0;
}
